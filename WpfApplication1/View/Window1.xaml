<Window x:Class="WpfApplication1.View.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1.View"
        mc:Ignorable="d"
        Title="Window1" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <Button x:Name="navigationwindows" Content="navigation windows" Click="navigationwindows_Click" MouseEnter="navigationwindows_MouseEnter"/>
            <Button x:Name="navigatetobase" Content="navigate to base" Click="navigatetobase_Click"/>
            <Button x:Name="navigatetoframe" Content="navigate to frame" Click="navigatetoframe_Click" />
        </StackPanel>
    </Grid>
</Window>
